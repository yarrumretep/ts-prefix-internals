{
  "name": "ts-prefix-internals",
  "version": "0.1.0",
  "description": "TypeScript symbol prefixer for aggressive minification â€” prefix internal symbols with _ so terser/esbuild can mangle them via mangleProps: /^_/",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "ts-prefix-internals": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "release": "npm test && npm version patch && git push origin main --tags && gh release create v$(node -p \"require('./package.json').version\") --generate-notes && npm publish"
  },
  "keywords": [
    "typescript",
    "mangling",
    "minification",
    "terser",
    "esbuild",
    "prefix",
    "internal",
    "symbol"
  ],
  "author": "Peter Murray",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yarrumretep/ts-prefix-internals.git"
  },
  "homepage": "https://github.com/yarrumretep/ts-prefix-internals#readme",
  "bugs": {
    "url": "https://github.com/yarrumretep/ts-prefix-internals/issues"
  },
  "dependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "vitest": "^3.0.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.0.0"
  }
}
